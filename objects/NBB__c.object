        <description>changed per Sifter #66</description>
        <externalId>false</externalId>
        <formula>IF(
AND(
NA_Date__c &gt;= (TODAY()-(MOD(TODAY() - (DATE (2011 ,7, 18)) ,7))), NA_Date__c &lt;=TODAY(), YTD_Binder_Date__c&lt;=TODAY() ), NBB__c ,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>WTD NBB Credit</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
