    <fields>
        <fullName>YTD_Binder_Date__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
	AND(
		(MONTH(Binder_Date__c) = 2), 
		(DAY(Binder_Date__c) = 29)
	),
	DATE( 
		YEAR( TODAY() ) 
		+ 
		(
			YEAR(Binder_Date__c) 
			- 
			YEAR(NA_Date__c)
		) , 
		2 , 
		28
	) ,
	DATE( 
		YEAR( TODAY() ) 
		+ 
		(
			YEAR(Binder_Date__c) 
			- 
			YEAR(NA_Date__c)
		) , 
		MONTH( Binder_Date__c ) , 
		Day(Binder_Date__c)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>YTD Binder Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>YTD_Date__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
	AND((MONTH(NA_Date__c) = 2), 
		(DAY(NA_Date__c) = 29)
		),
	DATE( YEAR( TODAY() ) , 2 , 28) ,
	DATE( YEAR( TODAY() ) , MONTH( NA_Date__c ) , Day(NA_Date__c))
	)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>YTD Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
