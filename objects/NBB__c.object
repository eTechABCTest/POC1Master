&quot;Yes&quot;,
IF(MONTH(TODAY()) &gt;= 4 &amp;&amp; MONTH(TODAY()) &lt;= 6 &amp;&amp; MONTH(NA_Date__c) &gt;= 4 &amp;&amp; MONTH(NA_Date__c) &lt;= 6,
&quot;Yes&quot;,
IF(MONTH(TODAY()) &gt;= 7 &amp;&amp; MONTH(TODAY()) &lt;= 9 &amp;&amp; MONTH(NA_Date__c) &gt;= 7 &amp;&amp; MONTH(NA_Date__c) &lt;= 9,
&quot;Yes&quot;,
IF(MONTH(TODAY()) &gt;= 10 &amp;&amp; MONTH(TODAY()) &lt;= 12 &amp;&amp; MONTH(NA_Date__c) &gt;= 10 &amp;&amp; MONTH(NA_Date__c) &lt;= 12,
&quot;Yes&quot;,
&quot;No&quot;)
)
)
),
&quot;No&quot;)</formula>
        <label>NA Date Current Quarter</label>
