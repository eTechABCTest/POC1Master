    <fields>
        <fullName>YTD_Date_in_Current_Quarter__c</fullName>
        <externalId>false</externalId>
        <formula>CASE(MONTH(YTD_Date__c),
1, CASE(MONTH(TODAY()), 1, 1, 2, 1, 3, 1, 0),
2, CASE(MONTH(TODAY()), 1, 1, 2, 1, 3, 1, 0),
3, CASE(MONTH(TODAY()), 1, 1, 2, 1, 3, 1, 0),
4, CASE(MONTH(TODAY()), 4, 1, 5, 1, 6, 1, 0),
5, CASE(MONTH(TODAY()), 4, 1, 5, 1, 6, 1, 0),
6, CASE(MONTH(TODAY()), 4, 1, 5, 1, 6, 1, 0),
7, CASE(MONTH(TODAY()), 7, 1, 8, 1, 9, 1, 0),
8, CASE(MONTH(TODAY()), 7, 1, 8, 1, 9, 1, 0),
9, CASE(MONTH(TODAY()), 7, 1, 8, 1, 9, 1, 0),
10, CASE(MONTH(TODAY()), 10, 1, 11, 1, 12, 1, 0),
11, CASE(MONTH(TODAY()), 10, 1, 11, 1, 12, 1, 0),
12, CASE(MONTH(TODAY()), 10, 1, 11, 1, 12, 1, 0),
0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
