    <label>Incentive Change Batch</label>
    <listViews>
        <fullName>All</fullName>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <listViews>
        <fullName>Current_Incentive_Change_Batch</fullName>
        <columns>NAME</columns>
        <columns>Applied__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Applied__c</field>
            <operation>equals</operation>
            <value>TODAY</value>
        </filters>
        <label>Current Incentive Change Batch</label>
    </listViews>
    <listViews>
        <fullName>Future_Incentive_Change_Batch</fullName>
        <columns>NAME</columns>
        <columns>Applied__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Applied__c</field>
            <operation>greaterThan</operation>
            <value>TODAY</value>
        </filters>
        <label>Future Incentive Change Batch</label>
    </listViews>
    <listViews>
        <fullName>Past_Incentive_Change_Batch</fullName>
        <columns>NAME</columns>
        <columns>Applied__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Applied__c</field>
            <operation>lessThan</operation>
            <value>TODAY</value>
        </filters>
        <label>Past Incentive Change Batch</label>
    </listViews>
    <nameField>
