    <validationRules>
        <fullName>VR_Advisory_Profile_Req_New_OSR</fullName>
        <active>true</active>
        <description>Created for CR-20151013-9317.</description>
        <errorConditionFormula>AND( 
	RecordType.DeveloperName  = &quot;Credit_Card_Request&quot;, 
	ISPICKVAL(Request_Type__c, &quot;New ABC Credit Card&quot;),
	OR(
          ISPICKVAL(Status__c, &quot;In Progress&quot;),
          ISPICKVAL(Status__c, &quot;Complete&quot;)
        ),
	ISBLANK(Advisory_Profile__c) 
)</errorConditionFormula>
        <errorDisplayField>Advisory_Profile__c</errorDisplayField>
        <errorMessage>Advisory Profile must be populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_ValidateAddressForExpediatedDelivery</fullName>
        <active>true</active>
        <description>Created for CR-20140515-5525</description>
        <errorConditionFormula>AND(
	RecordType.DeveloperName = &quot;Credit_Card_Request&quot;,
	OR(
		ISPICKVAL(Request_Type__c, &quot;Replacement ABC Credit Card (Alternate Delivery)&quot;),
		ISPICKVAL(Expedited_Processing_Approval__c, &quot;Approved&quot;)
	),
	OR(
		ISNULL(Address_To_Name__c),
		ISBLANK(Address_To_Name__c),
		ISNULL(Address_Line_1__c),
		ISBLANK(Address_Line_1__c),
		ISNULL(Address_City__c),
		ISBLANK(Address_City__c),
		ISNULL(Address_State__c),
		ISBLANK(Address_State__c),
		ISNULL(Address_Postal_Code__c),
		ISBLANK(Address_Postal_Code__c)
	)
)</errorConditionFormula>
        <errorDisplayField>Address_City__c</errorDisplayField>
        <errorMessage>A full Address must be specified for requests with Expedited shipping.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_ValidateRelatedCard</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISBLANK(Related_Card__c),
 OR(
  CONTAINS( TEXT(Request_Type__c), &apos;Replacement&apos;),
  CONTAINS(TEXT(Request_Type__c), &apos;Deactivate&apos;)
 ) 
)</errorConditionFormula>
        <errorDisplayField>Related_Card__c</errorDisplayField>
        <errorMessage>Related Card is required for replace or deactivation request.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Approve_Card_Request</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Process Card Request</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var status = &apos;{!OSR__c.Status__c}&apos;;
var requestType = &apos;{!OSR__c.Request_Type__c}&apos;;
var verified = &apos;{!OSR__c.Agreement_Verified__c}&apos;;
var received = &apos;{!OSR__c.Agreement_Received__c}&apos;;
var relatedCard = &apos;{!OSR__c.Related_Card__c}&apos;;
var requestReason = &apos;{!OSR__c.Request_Reason__c}&apos;;
var expApproval = &apos;{!OSR__c.Expedited_Processing_Approval__c}&apos;;
var result;

// check for birthdate
{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/connection.js&quot;)}
var updateRecord = new Array(); 
var myquery = &quot;SELECT Id, Name, Birthdate FROM Contact WHERE Id = &apos;{!OSR__c.ContactId__c}&apos; limit 1&quot;;
result = sforce.connection.query(myquery);
records = result.getArray(&quot;records&quot;);
if (records.length == 0) {
    alert(&apos;Contact is required to approve a Card Request.&apos;);
}
else {
    if (records[0].Birthdate == null) {
        alert(records[0].Name + &apos;\&apos;s Workday profile does not contain a date of birth.  We ask for this information so in the event they need to call into our customer service center, we can uniquely identify them.  Ask &apos; + records[0].Name + &apos; to login to workday and enter their birthdate.  Once entered, please wait until the next business day to attempt to apply again.&apos;);
    }
    else {
        if(requestType == &apos;New ABC Credit Card&apos;){
	  if (verified == false) {
		alert(&apos;Card Agreement must be verified before request can be Approved.&apos;);
	  }
	  else
	  if (status != &apos;Pending Finance&apos;) {
		alert(&apos;Status must be in Pending Finance to Approve&apos;);
	  }
	  else
	  if (received == &apos;&apos;) {
		alert(&apos;Agreement Received must have a date value to Approve&apos;);
	  }
	  else {
		parent.location.href = (&apos;/apex/CreditRequest?OSRID={!OSR__c.Id}&amp;CID={!OSR__c.ContactId__c}&apos;);
	  }
	}
	else{
	if(requestType == &apos;Replacement ABC Credit Card&apos; || requestType == &apos;Replacement ABC Credit Card (Expedited)&apos;){

		//As this is a replacement request, address fields can&apos;t be blank.
		if(&apos;{!OSR__c.Address_To_Name__c}&apos; == null || &apos;{!OSR__c.Address_To_Name__c}&apos; == &apos;&apos; || &apos;{!OSR__c.Address_Line_1__c}&apos; == null || &apos;{!OSR__c.Address_Line_1__c}&apos; == &apos;&apos; || &apos;{!OSR__c.Address_City__c}&apos; == null || &apos;{!OSR__c.Address_City__c}&apos; == &apos;&apos; || &apos;{!OSR__c.Address_State__c}&apos; == null || &apos;{!OSR__c.Address_State__c}&apos; == &apos;&apos; || &apos;{!OSR__c.Address_Postal_Code__c}&apos; == null || &apos;{!OSR__c.Address_Postal_Code__c}&apos; == &apos;&apos; ) {
			alert(&apos;For replacement request, the following fields must all be filled in:  Address To Name, Address Line 1, Address City, Address State, Address Postal Code.&apos;);
		} else {
			if(requestType == &apos;Replacement ABC Credit Card (Expedited)&apos; &amp;&amp; expApproval == &apos;&apos;){
				alert(&apos;This is an Expedited Replacement request.  You must indicate if expedited shipping is approved in the field Expedited Processing Approval.&apos;);
			}
			else {
				parent.location.href = (&apos;/apex/CreditRequest?OSRID={!OSR__c.Id}&amp;CID={!OSR__c.ContactId__c}&apos;);
			}
		}
	}
	else
	if(requestType == &apos;Deactivate ABC Credit Card&apos;){ 
	  if(requestReason == &apos;&apos;) {
		alert(&apos;Request Reason must have a value to Approve&apos;);
	  }
	  else
		parent.location.href = (&apos;/apex/CreditRequest?OSRID={!OSR__c.Id}&amp;CID={!OSR__c.ContactId__c}&apos;);
	  }  
	}
    }
}</url>
