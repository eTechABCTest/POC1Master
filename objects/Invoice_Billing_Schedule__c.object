    <fields>
        <fullName>Schedule_Status__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Total_Negotiated_Amt__c = Line_Items_Total_Amt__c , &quot;All Amounts Scheduled&quot;,
IF(Total_Negotiated_Amt__c  &gt;  Line_Items_Total_Amt__c, &quot;ALERT: Additional Line Item Amounts Needed&quot;,
IF(Total_Negotiated_Amt__c &lt; Line_Items_Total_Amt__c, &quot;WARNING: Line Items exceed Negotiated Amounts&quot;, &quot; &quot;)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Schedule Status Text</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Total_Negotiated_Amt__c = Line_Items_Total_Amt__c , IMAGE(&quot;/img/samples/flag_green.gif&quot;,&quot;All Amounts Scheduled&quot;),
IF(Total_Negotiated_Amt__c &gt; Line_Items_Total_Amt__c, IMAGE(&quot;/img/samples/flag_yellow.gif&quot;,&quot;ALERT: Additional Line Item Amounts Needed&quot;),
IF(Total_Negotiated_Amt__c &lt; Line_Items_Total_Amt__c, IMAGE(&quot;/img/samples/flag_red.gif&quot;,&quot;WARNING: Line Items exceed Negotiated Amounts&quot;),&quot; &quot;)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Status</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
