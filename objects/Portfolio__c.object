        <externalId>false</externalId>
        <formula>IF( 
	OR(
		ISNULL( Override_Balance_Limit_Warning_Level__c ) ,  
		ISBLANK( Override_Balance_Limit_Warning_Level__c )
	), 
	Portfolio_Profile__r.Balance_Limit_Warning_Level__c , 
	Override_Balance_Limit_Warning_Level__c 
)</formula>
        <label>zzzBalance Limit Warning Level</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Balance_Over_Warning_Level__c</fullName>
        <externalId>false</externalId>
        <formula>AND( 
(Available_Balance_Percent__c &lt;= Balance_Limit_Warning_Level__c), 
(Active_Card_Count__c &gt; 0),
(zzzCredit_Limit__c &gt; 0) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>zzzBalance Over Warning Level</label>
