        <description>Used in reports and dashboards for grouping</description>
        <externalId>false</externalId>
        <formula>IF( Available_Balance_Percent__c &gt;  Balance_Limit_Warning_Level__c,
    &apos;Under&apos;,
    IF( Available_Balance_Percent__c &gt; 0,
        &apos;Warning&apos;,
        &apos;Over&apos; ) )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
