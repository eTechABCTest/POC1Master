    <fields>
        <fullName>Warning_Type__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
	AND(
		(Oldest_transaction__c + Advisory_Profile__r.Over_aging_Warning_days__c) &lt; TODAY(),
		(Oldest_transaction__c + Advisory_Profile__r.Over_aging_Warning_days_2__c) &gt;= TODAY()
	),
	&apos;First&apos;,
	IF(
		AND(
			(Oldest_transaction__c + Advisory_Profile__r.Over_aging_Warning_days_2__c) &lt; TODAY(),
			(Oldest_transaction__c + Advisory_Profile__r.Over_aging_Warning_days_3__c) &gt;= TODAY()
		),
		&apos;Second&apos;,
		IF(
			AND(
				(Oldest_transaction__c + Advisory_Profile__r.Over_aging_Warning_days_3__c) &lt; TODAY(),
				(Oldest_transaction__c + Advisory_Profile__r.Over_aging_Maximum_days__c) &gt;= TODAY()
			),
			&apos;Third&apos;,
			IF(
				AND(
					(Oldest_transaction__c + Advisory_Profile__r.Over_aging_Maximum_days__c) &lt; TODAY()
				),
				&apos;Deactivate&apos;,
				&apos;&apos;
			)
		)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Warning Type</label>
