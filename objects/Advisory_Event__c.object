    <validationRules>
        <fullName>Cannot_close_with_Registrations</fullName>
        <active>true</active>
        <description>If Status = “Closed - Completed” and Registrations to Date are &gt; 0</description>
        <errorConditionFormula>AND( 
    ISPICKVAL(Status__c, &quot;Closed - Completed&quot;),
     Registrations_Total_to_Date__c &gt; 0
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>You cannot Close this event without first indicating the attendees.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enforce_Parent</fullName>
        <active>true</active>
        <description>Session Events must identify a Parent Event</description>
        <errorConditionFormula>AND(
    RecordType.Name = &apos;Session&apos;,
    ISBLANK( Parent_Event__c)
)</errorConditionFormula>
        <errorMessage>You must identify a parent</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Event_Date</fullName>
        <active>true</active>
        <description>Date Begin must be prior to Date End.</description>
        <errorConditionFormula>Date_Begins__c &gt;  Date_Ends__c</errorConditionFormula>
        <errorDisplayField>Date_Begins__c</errorDisplayField>
        <errorMessage>Date Begins must be prior to Date Ends .</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Event_Date_Time</fullName>
        <active>true</active>
        <description>Begins must be prior to Ends Date.</description>
        <errorConditionFormula>Begins__c &gt; Ends__c</errorConditionFormula>
        <errorDisplayField>Begins__c</errorDisplayField>
        <errorMessage>Begins must be prior to Ends.</errorMessage>
    </validationRules>
