    <fields>
        <fullName>In_Sync__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
	OR(
		AND(
			ISPICKVAL(Status__c, &apos;Active&apos;),
			ISPICKVAL(Comdata_Status__c, &apos;A&apos;)
		),
		AND(
			OR(
				ISPICKVAL(Comdata_Status__c, &apos;B&apos;),
				ISPICKVAL(Comdata_Status__c, &apos;C&apos;),
				ISPICKVAL(Comdata_Status__c, &apos;F&apos;),
				ISPICKVAL(Comdata_Status__c, &apos;L&apos;),
				ISPICKVAL(Comdata_Status__c, &apos;S&apos;)
			),
			OR(
				ISPICKVAL(Status__c, &apos;Inactive&apos;),
				ISPICKVAL(Status__c, &apos;Deactivated&apos;)
			)
		)
	),
	IMAGE(&apos;/resource/1415977293000/correct&apos;, &apos;In Sync&apos;),
	IMAGE(&apos;/resource/1415977293000/wrong&apos;, &apos;Not In Sync&apos;)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>In Sync?</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
