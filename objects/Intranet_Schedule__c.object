    <validationRules>
        <fullName>Access_Level_Not_Allowed_Importance_L1</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(Importance__c, &apos;L1&apos;), NOT($Setup.Intranet_Constants__c.Allow_Importance_L1__c))</errorConditionFormula>
        <errorDisplayField>Importance__c</errorDisplayField>
        <errorMessage>Your access level does not allow an item to be scheduled at Importance “L1”</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approve_Intranet_Schedule</fullName>
        <active>true</active>
        <description>Only specific named users are allowed to approve or reject Schduled content</description>
        <errorConditionFormula>AND(OR(ISPICKVAL(Approval_Status__c,&apos;Approved&apos;),ISPICKVAL(Approval_Status__c,&apos;Rejected&apos;)),$Setup.Intranet_Constants__c.Allow_Schedule_Approval__c = False)</errorConditionFormula>
        <errorDisplayField>Approval_Status__c</errorDisplayField>
        <errorMessage>Your access level does not allow Approval or Rejection of a scheduled item</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_specify_timeframe_emphasis</fullName>
        <active>true</active>
        <description>Prevent an item from being emphasized over a timeframe if there is no start and/or end date</description>
        <errorConditionFormula>AND(
OR(ISBLANK(Start_Date_Time__c), ISBLANK(End_Date_Time__c)),
NOT(ISBLANK(TEXT(Timeline_Emphasis__c)))
)</errorConditionFormula>
        <errorMessage>Cannot specify Timeline Emphasis if both Start and End Date are not Specified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Required_to_Approve_Date_Based_Schd</fullName>
        <active>true</active>
        <description>Require either a Start Date or End Date on a Date Based Schedule record for Approval</description>
        <errorConditionFormula>AND(
RecordType.Name = &quot;Date-Based&quot;,
ISPICKVAL(Approval_Status__c, &quot;Approved&quot;),
ISBLANK(Start_Date_Time__c),
ISBLANK( End_Date_Time__c)
)</errorConditionFormula>
        <errorMessage>A Start Date or End Date is required before Approving a Date-Based type Schedule</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Office_Required</fullName>
        <active>true</active>
        <description>A selection for Office is required for specific item types</description>
        <errorConditionFormula>AND( OR(ISPICKVAL(Intranet_Item__r.Item_Type__c,&apos;News Item&apos;),ISPICKVAL(Intranet_Item__r.Item_Type__c,&apos;Office Event&apos;)), ISBLANK( Office__c ))</errorConditionFormula>
        <errorMessage>Office must be specified for a News Item Schedule.  If the content is for all offices, select &apos;Firmwide&apos;</errorMessage>
    </validationRules>
