    <fields>
        <fullName>Is_Visible__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
  AND(
    ISPICKVAL(Active_Status__c,&apos;Active&apos;),
    ISPICKVAL( Approval_Status__c ,&apos;Approved&apos;),
    IF(NOT(ISBLANK(Start_Date_Time__c)),NOW() &gt; Start_Date_Time__c,TRUE), 
    IF(NOT(ISBLANK(End_Date_Time__c)),NOW() &lt; End_Date_Time__c,TRUE),
    Intranet_Item__r.Is_Visible__c = TRUE
  ),
  TRUE,
  FALSE
)</formula>
        <inlineHelpText>Visible if Active, Approved, with date range (Start/End Dates) and if related Item is visible</inlineHelpText>
