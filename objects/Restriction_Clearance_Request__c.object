    <validationRules>
        <fullName>Prevent_New_Request_on_Retired</fullName>
        <active>true</active>
        <description>New requests cannot be saved on a retired Restriction.</description>
        <errorConditionFormula>AND(
ISNEW(),
Restriction__r.Status__c = &apos;Retired&apos;
)</errorConditionFormula>
        <errorMessage>New requests cannot be saved on a retired Restriction.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restriction_Clearance_Dates_Required</fullName>
        <active>true</active>
        <description>Created as part of CR-7501. Please enter a value in either Date: Last Marketing Outreach to Contact or Date: Last Research Interaction before trying to save the record.</description>
        <errorConditionFormula>ISBLANK(DateLastMarketingOutreachtoContact__c) &amp;&amp;
ISBLANK(Date_Last_Research_Interaction__c)</errorConditionFormula>
        <errorMessage>Please enter a value in either Date: Last Marketing Outreach to Contact or Date: Last Research Interaction before trying to save the record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restriction_Clearance_Marketer_Required</fullName>
        <active>true</active>
        <description>Created as part of CR-7501</description>
        <errorConditionFormula>NOT(ISPICKVAL(Reason_for_Clearance_Request__c, &quot;Research Request&quot;)) &amp;&amp; ISBLANK(Marketer__c)</errorConditionFormula>
        <errorDisplayField>Marketer__c</errorDisplayField>
        <errorMessage>Please enter a value in the Marketer field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_ValidateUserForNotifyCreatorOfRespons</fullName>
        <active>true</active>
        <description>CR-3014</description>
        <errorConditionFormula>AND(
                NOT(ISNEW()),
                $User.Id != CreatedById,
                ISCHANGED(Notify_Creator_of_Response__c)
)</errorConditionFormula>
        <errorDisplayField>Notify_Creator_of_Response__c</errorDisplayField>
        <errorMessage>This field can be updated only by record creator.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Warm_Lead_Required</fullName>
        <active>true</active>
        <description>CR-20120924-1526</description>
        <errorConditionFormula>AND(
		CONTAINS(TEXT(Reason_for_Clearance_Request__c),&apos;Warm Lead&apos;), 
		OR(
			AND(
				ISNEW(),
				TODAY() &gt; DATE(2012,12,14)
			),
			DATEVALUE(CreatedDate) &gt; DATE(2012,12,14)
		),
		ISBLANK(Warm_Lead__c)
)</errorConditionFormula>
        <errorMessage>The &quot;Warm Lead&quot; field is required if the Reason for Clearance Request field contains &quot;Warm Lead&quot;.</errorMessage>
    </validationRules>
</CustomObject>
