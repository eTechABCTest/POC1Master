    <validationRules>
        <fullName>VR_Base_Process</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(
	TEXT(Base_Process__c)
)</errorConditionFormula>
        <errorDisplayField>Base_Process__c</errorDisplayField>
        <errorMessage>You must supply a Base Process</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_No_Discount_Allowed</fullName>
        <active>true</active>
        <description>Discount Type is not “Explicit”</description>
        <errorConditionFormula>AND(
	NOT(ISPICKVAL(
			Discount_Type__c,
			&apos;Explicit&apos;)
	),
	Discount__c &gt; 0
)</errorConditionFormula>
        <errorDisplayField>Discount__c</errorDisplayField>
        <errorMessage>The Discount % must be blank or zero if the Discount Type is not explicit.</errorMessage>
    </validationRules>
