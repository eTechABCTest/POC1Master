    <validationRules>
        <fullName>Job_function_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    ISBLANK( TEXT( Job_Function__c ) ),
     RecordType.Name == &quot;Title&quot;
)</errorConditionFormula>
        <errorDisplayField>Job_Function__c</errorDisplayField>
        <errorMessage>You must select a Job Function</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Program_is_required</fullName>
        <active>true</active>
        <description>Program is required when record type is Program and Program field is blank.</description>
        <errorConditionFormula>AND(
    RecordType.Name == &quot;Program&quot;,
    ISBLANK( Program__c )
)</errorConditionFormula>
        <errorDisplayField>Program__c</errorDisplayField>
        <errorMessage>You must select a Program</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Threshold_type_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    RecordType.Name == &quot;Threshold&quot;,
    ISBLANK( TEXT( Threshold_Type__c ) )
)</errorConditionFormula>
        <errorDisplayField>Threshold_Type__c</errorDisplayField>
        <errorMessage>You must declare a threshold type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Threshold_value_is_required</fullName>
        <active>true</active>
        <description>You must define the threshold as either a number or percentage</description>
        <errorConditionFormula>AND(
    OR(
        AND(
            ISBLANK( Threshold_Number__c ),
            ISBLANK( Threshold_Percent__c )
        ),
        AND(
            NOT( ISBLANK( Threshold_Number__c ) ),
            NOT( ISBLANK( Threshold_Percent__c ) )
        )
    ),
     RecordType.Name == &quot;Threshold&quot;
)</errorConditionFormula>
        <errorMessage>You must define the threshold as either a number or percentage</errorMessage>
    </validationRules>
</CustomObject>
