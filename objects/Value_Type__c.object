            INCLUDES(Values_Permitted__c, &apos;Program Collection&apos;),
            OR(
                INCLUDES(Values_Permitted__c, &apos;Date&apos;),
		INCLUDES(Values_Permitted__c, &apos;Number&apos;),
		INCLUDES(Values_Permitted__c, &apos;Percentage&apos;),
