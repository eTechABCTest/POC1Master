    <validationRules>
        <fullName>Campaign_Equals_Web_Application_Req</fullName>
        <active>true</active>
        <description>IF Campaign (New) = â€œWeb Applicationâ€ Contact Quantity must be &lt; 20</description>
        <errorConditionFormula>AND( 
ISPICKVAL(Campaign_New__c, &quot;Web Application&quot;),  
OR(ISBLANK(Contact_Quantity__c ), Contact_Quantity__c &gt;= 20 )
)</errorConditionFormula>
        <errorMessage>If Campaign (New) is equal to â€œWeb Applicationâ€, then the Contact Quantity must be &lt; 20</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Campaign_Equals_eQual_Req</fullName>
        <active>false</active>
        <description>Per A.Sutton: 1.20.16 &apos;..change this to remove the ERROR message when the â€œcampaign (new)â€ type is â€œeQualâ€?..&apos;</description>
        <errorConditionFormula>AND( 
BEGINS(TEXT(Campaign_New__c),  &quot;eQual&quot;) ,  
OR(ISBLANK(Contact_Quantity__c ),Contact_Quantity__c &lt;= 20)
)</errorConditionFormula>
        <errorMessage>If the Campaign (New) starts with â€˜eQualâ€™, then Contact Quantity must be &gt; 20</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_the_Record_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	$Profile.Name != &apos;System Administrator&apos;,
	NOT(ISCHANGED(Actual_Quantity_DW__c)),
	AND(
	    Lock_the_Record__c,
	    NOT(ISCHANGED(Lock_the_Record__c))
	)
)</errorConditionFormula>
        <errorMessage>This Record is locked and cannot be edited</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Program_Status_Contracted_Req</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISPICKVAL(Status__c ,&quot;Contracted&quot;), 
 OR(
  ISBLANK( Contract_Sent__c ),
  ISBLANK( Proposal_Sent__c ),
  ISBLANK(Contract_Signature_Date__c )
 )
)</errorConditionFormula>
        <errorDisplayField>Actual_Program_Deployment_Date_R__c</errorDisplayField>
        <errorMessage>When program status is &quot;Contracted&quot;, the contract sent date, proposal sent date, and signature date is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Program_Status_Declined_Req</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISPICKVAL(Status__c ,&quot;Declined&quot;), 
 OR(
  ISBLANK( Declined_Date__c ),
  ISBLANK( Proposal_Sent__c )
 )
)</errorConditionFormula>
        <errorMessage>When program status is &quot;Declined&quot;, the declined date and proposal sent date is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Program_Status_Opportunity_Req</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
 ISPICKVAL(Status__c ,&quot;Opportunity&quot;),
 ISBLANK(Probability__c) 
)</errorConditionFormula>
        <errorDisplayField>Probability__c</errorDisplayField>
        <errorMessage>When program status is &quot;Opportunity&quot;, the probability is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Program_Status_Pending_Signature_Req</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISPICKVAL(Status__c ,&quot;Pending Signature&quot;), 
 OR(
  ISBLANK( Contract_Sent__c ),
  ISBLANK( Proposal_Sent__c )
 )
)</errorConditionFormula>
        <errorMessage>When program status is &quot;Pending Signature&quot;, the contract sent date and proposal sent date is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Program_Status_Proposed_Req</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISPICKVAL(Status__c ,&quot;Proposed&quot;), 
 OR(
  ISBLANK(Probability__c),
  ISBLANK( Proposal_Sent__c )
 )
)</errorConditionFormula>
        <errorMessage>When program status is &quot;Proposed&quot;, the probability and proposal sent date is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_field_validation</fullName>
        <active>true</active>
        <description>Only those individuals with &quot;invoicing accessâ€ are able to change to the value to â€œContractedâ€ and â€œPending Signature&quot;.Contact Accounting to make this update.</description>
        <errorConditionFormula>ISCHANGED(Status__c) &amp;&amp; (ISPICKVAL(Status__c, &quot;Contracted&quot;) || ISPICKVAL(Status__c,&quot;Pending Signature&quot;)) &amp;&amp; !$Permission.Invoicing_access</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Only those individuals with â€œinvoicing accessâ€ are able to change to the value to â€œContractedâ€ and â€œPending Signatureâ€. Contact Accounting to make this update</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_CONTACT_QUANTITY_VALUE</fullName>
        <active>true</active>
        <description>Contact Quantity value should be equals to sum of Quantity Cost Details values.</description>
        <errorConditionFormula>AND(
	OR(
		DATEVALUE(CreatedDate) &gt;=  DATE(2016,4,28),
		ISNEW()
	),
	NOT(ISNULL(Campaign_SubType__c)),
        OR(
	INCLUDES(Campaign_SubType__c, &apos;AM: IP Name&apos;),
	INCLUDES(Campaign_SubType__c, &apos;AM: New Name&apos;),
	INCLUDES(Campaign_SubType__c, &apos;AM: PRE Name&apos;),
	INCLUDES(Campaign_SubType__c, &apos;SE: PRE Name&apos;),
	INCLUDES(Campaign_SubType__c, &apos;SE: IP Name (Client-Provided)&apos;),
	INCLUDES(Campaign_SubType__c, &apos;SE: IP Name (Royall-Generated)&apos;),
	INCLUDES(Campaign_SubType__c, &apos;SE: New Name&apos;),
	INCLUDES(Campaign_SubType__c, &apos;SS/PRE: New Name&apos;),
	INCLUDES(Campaign_SubType__c, &apos;SS/PRE: PRE Name&apos;)
        ),
	Contact_Quantity__c
	!=
	IF(ISNULL(QTY_AM_IP_Name__c), 0, QTY_AM_IP_Name__c)
	+ 
	IF(ISNULL(QTY_AM_new_name__c), 0, QTY_AM_new_name__c)
	+
	IF(ISNULL(QTY_AM_PRE_Name__c), 0, QTY_AM_PRE_Name__c)
	+
	IF(ISNULL(QTY_SE_IP_Name_Client__c), 0, QTY_SE_IP_Name_Client__c)
	+ 
	IF(ISNULL(QTY_SE_IP_Name_Royall__c), 0, QTY_SE_IP_Name_Royall__c)
	+
	IF(ISNULL(QTY_SE_New_Name__c), 0, QTY_SE_New_Name__c)
	+
	IF(ISNULL(QTY_SE_PRE_Name__c), 0, QTY_SE_PRE_Name__c)
	+ 
	IF(ISNULL(QTY_SS_New_Name__c), 0, QTY_SS_New_Name__c)
	+
	IF(ISNULL(QTY_SS_PRE_Name__c), 0, QTY_SS_PRE_Name__c)
)</errorConditionFormula>
        <errorDisplayField>Contact_Quantity__c</errorDisplayField>
        <errorMessage>Contact Quantity value should be equal to sum of Quantity Cost Details values.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_VALIDATE_POSTAGE_AND_PROGRAM_COST</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 OR(
    DATEVALUE(CreatedDate) &gt;=  DATE(2016,4,28),
        ISNEW()
    ),
    NOT(ISNULL(Campaign_SubType__c)),
    OR(
        INCLUDES(Campaign_SubType__c, &apos;SS/PRE: New Name&apos;),
        INCLUDES(Campaign_SubType__c, &apos;SS/PRE: PRE Name&apos;)
    ),
    OR(
        (
            IF(ISNULL(Program_Cost__c), 0, Program_Cost__c)
                != 
            (
                IF(ISNULL(Program_Cost_SS_New__c), 0, Program_Cost_SS_New__c)
                    +
                IF(ISNULL(Program_Cost_SS_PRE__c), 0, Program_Cost_SS_PRE__c)
                
            )
        ),
        (
            IF(ISNULL(Postage_Cost__c), 0, Postage_Cost__c)
                != 
            (
                IF(ISNULL(Postage_Cost_SS_New__c), 0, Postage_Cost_SS_New__c)
                    + 
                IF(ISNULL(Postage_Cost_SS_PRE__c), 0, Postage_Cost_SS_PRE__c)
            )
        )
)
)</errorConditionFormula>
        <errorMessage>Program Cost and Postage Cost should be aggregate of corresponding fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Probability</fullName>
        <active>true</active>
        <errorConditionFormula>Probability__c &gt; 1 ||  Probability__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Probability__c</errorDisplayField>
        <errorMessage>The probability must be between 0% - 100%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Date_Contract_Proposed</fullName>
        <active>true</active>
        <description>Proposed Date â‰¤ Contract Sent Date â‰¤ Contract Signed Date</description>
        <errorConditionFormula>AND(   NOT(ISBLANK(Contract_Signature_Date__c)), 
             NOT(ISBLANK(Contract_Sent__c )),
             (Contract_Signature_Date__c  &lt; Contract_Sent__c)
)
||
AND(  NOT(ISBLANK(Proposal_Sent__c)), 
            NOT(ISBLANK(Contract_Sent__c )), 
            (Contract_Sent__c &lt; Proposal_Sent__c) 
)
||
AND(  NOT(ISBLANK(Proposal_Sent__c)),
            NOT(ISBLANK(Contract_Signature_Date__c)),
            (Contract_Signature_Date__c &lt; Proposal_Sent__c)
)</errorConditionFormula>
        <errorMessage>Contract Signed Date must be after Contract Sent Date which must be after Proposed Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Date_Declined_Proposed</fullName>
        <active>true</active>
        <description>Proposed Date â‰¤ Declined Date</description>
        <errorConditionFormula>NOT(Declined_Date__c &gt;= Proposal_Sent__c)</errorConditionFormula>
        <errorDisplayField>Actual_Program_Deployment_Date_R__c</errorDisplayField>
        <errorMessage>Declined Date must be after Proposed Date</errorMessage>
    </validationRules>
