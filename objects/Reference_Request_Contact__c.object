    <validationRules>
        <fullName>Status_must_be_Approved_before_Released</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  ISPICKVAL( Approval_Status__c , &quot;Released&quot;) ,
  ISCHANGED( Approval_Status__c ),
  NOT( 
    ISPICKVAL( PRIORVALUE(Approval_Status__c) , &quot;Approved&quot;) 
  )
)</errorConditionFormula>
        <errorDisplayField>Approval_Status__c</errorDisplayField>
        <errorMessage>Approval Status must be Approved prior to Released.</errorMessage>
    </validationRules>
