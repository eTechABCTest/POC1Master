    <fields>
        <fullName>Value__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
	RecordType.DeveloperName = &apos;Collection_Perspective&apos;, 
	IF(
		ISBLANK(Collection_Perspective__c), 
		Individual_Program__r.Name, 
		Collection_Perspective__r.Name 
	),
	IF(
		RecordType.DeveloperName = &apos;Assignment_Role&apos;,
		Assignment_Role__r.Name,
		IF(
			RecordType.DeveloperName = &apos;Institution&apos;,
			Institution__r.Account_Formal_Name__c,
			IF(
				RecordType.DeveloperName = &apos;Relationship&apos;,
				IF(
					ISBLANK(To_Relationship__c),
					From_Relationship__r.Name, 
					To_Relationship__r.Name
				),
				IF(
					RecordType.DeveloperName = &apos;Renewal_Type&apos;,
					TEXT(Agreement_Type__c),
					IF(
						RecordType.DeveloperName = &apos;Activity_Date&apos;,
						TEXT(Date_Type__c) + &apos; &apos; + TEXT(Start_Date__c) + &apos; - &apos; + TEXT(End_Date__c),
						IF(
							RecordType.DeveloperName = &apos;Credit_Type&apos;,
							Credit_Type__r.Name,
							IF(
								RecordType.DeveloperName = &apos;Staff_Member&apos;,
								Staff_Member__r.FirstName + &apos; &apos; +  Staff_Member__r.LastName,
								&apos;&apos;
							)
						)
					)

				)

			)
		)
	)
)</formula>
        <label>Value</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
