    <validationRules>
        <fullName>Check_the_Env_is_correct_when_marked_com</fullName>
        <active>true</active>
        <description>When marked complete in Env, check if parent (Pre-Req) record is marked complete for same Env</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Prerequisite_Action_Item__c)),
OR(
ISPICKVAL(Step_Complete_in_Alternate__c, &quot;Yes&quot;),
ISPICKVAL(Step_Complete_in_Dev__c, &quot;Yes&quot;),
ISPICKVAL(Step_Complete_in_Production__c, &quot;Yes&quot;),
ISPICKVAL(Step_Complete_in_QA__c, &quot;Yes&quot;),
ISPICKVAL(Step_Complete_in_Rehearse__c, &quot;Yes&quot;),
ISPICKVAL(Step_Complete_in_UAT__c, &quot;Yes&quot;)),
OR(
TEXT(Step_Complete_in_Alternate__c) !=  TEXT(Prerequisite_Action_Item__r.Step_Complete_in_Alternate__c), 
TEXT(Step_Complete_in_Dev__c) !=  TEXT(Prerequisite_Action_Item__r.Step_Complete_in_Dev__c),
TEXT(Step_Complete_in_Production__c) !=  TEXT(Prerequisite_Action_Item__r.Step_Complete_in_Production__c),
TEXT(Step_Complete_in_QA__c) !=  TEXT(Prerequisite_Action_Item__r.Step_Complete_in_QA__c),
TEXT(Step_Complete_in_Rehearse__c) !=  TEXT(Prerequisite_Action_Item__r.Step_Complete_in_Rehearse__c),
TEXT(Step_Complete_in_UAT__c) !=  TEXT(Prerequisite_Action_Item__r.Step_Complete_in_UAT__c))
)</errorConditionFormula>
        <errorMessage>If the Step is marked as Complete then any Prerequisite step should be marked as complete as well in the necessary Environments.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Depl_Plan_Step_Not_Complete_Until_Prod</fullName>
        <active>true</active>
        <description>A Deployment Plan Step Action Item cannot be marked as Complete unless it has been marked as completed in Production</description>
        <errorConditionFormula>AND(Complete__c, TEXT(Step_Complete_in_Production__c)=&apos;No&apos;)</errorConditionFormula>
        <errorMessage>This Deployment Plan Step can only be marked as Completed once the step has been marked as Complete in Production</errorMessage>
    </validationRules>
