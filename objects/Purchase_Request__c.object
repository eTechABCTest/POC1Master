    <validationRules>
        <fullName>AutoRenewal_Date_Required</fullName>
        <active>true</active>
        <description>Require value if Auto_Renew__c is TRUE</description>
        <errorConditionFormula>Auto_Renew__c = TRUE &amp;&amp; ISBLANK ( Auto_Renewal_Date__c )</errorConditionFormula>
        <errorDisplayField>Auto_Renewal_Date__c</errorDisplayField>
        <errorMessage>Auto Renew is checked so a date is required for Auto Renewal Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contract_must_be_within_3_years</fullName>
        <active>true</active>
        <description>The contract must be within a 3 year window.</description>
        <errorConditionFormula>(Term_End_Date__c - Term_Start_Date__c)  &gt;  1095</errorConditionFormula>
        <errorMessage>The Contract must be within a 3 year window to be included in a Purchase Request. Please modify your Term Start Date or Term End Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Deadline_Date_and_Deadline_Reason_Req</fullName>
        <active>true</active>
        <description>Both fields are required if Deadline Type = &quot;Hard Deadline&quot;</description>
        <errorConditionFormula>ISPICKVAL(  Deadline_Type__c , &quot;Hard Deadline&quot;) &amp;&amp;
OR( ISBLANK ( Deadline_Date__c ), ISBLANK ( Deadline_Reason__c ))</errorConditionFormula>
        <errorMessage>If Deadline Type = Hard Deadline, then both Deadline Date and Deadline Reason are required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Vendor_must_be_approved_Purchase_Req</fullName>
        <active>true</active>
        <description>The Vendor must be approved before a Purchase Request can be created</description>
        <errorConditionFormula>(ISNEW () || ISCHANGED(Vendor__c)) &amp;&amp; (TEXT(Vendor__r.Status__c) = &quot;New - Pending Approval&quot; || TEXT(Vendor__r.Status__c) = &quot;Prohibited&quot;)</errorConditionFormula>
        <errorMessage>The Vendor associated with this Purchase Request has not been approved. Please ensure it is approved before trying to create a Purchase Request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Waiver_Reasons_required</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 
ISPICKVAL(X1_Termination_for_Convenience__c, &quot;No&quot;) &amp;&amp; ISBLANK( X1_Waiver_Reason__c), 
ISPICKVAL(X2_Fixed_Price_Increases__c, &quot;No&quot;) &amp;&amp; ISBLANK(  X2_Waiver_Reason__c), 
ISPICKVAL(X3_Contract_Auto_Renews__c, &quot;No&quot;) &amp;&amp; ISBLANK(  X3_Waiver_Reason__c ), 
ISPICKVAL(X4_Minimum_Commitments__c, &quot;No&quot;) &amp;&amp; ISBLANK(  X4_Waiver_Reason__c ) 
)</errorConditionFormula>
        <errorMessage>If 1. Termination for Convenience, 2. Fixed Price Increases, 3. Contract Auto-Renews or 4. Minimum Commitments equal No, then the corresponding numerical matching Waiver Reason must be completed.</errorMessage>
    </validationRules>
</CustomObject>
