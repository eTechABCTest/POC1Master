    <validationRules>
        <fullName>Approval_Status_Reason_Required</fullName>
        <active>true</active>
        <description>Approval Status Reason is Required if Approval Status = Denied or Delayed</description>
        <errorConditionFormula>AND(
  OR(ISPICKVAL( Approval_to_Vet__c , &quot;Denied&quot;),
  ISPICKVAL( Approval_to_Vet__c , &quot;Delayed&quot;)),
  ISBLANK(  Approval_Status_Reason__c  )
)</errorConditionFormula>
        <errorDisplayField>Approval_Status_Reason__c</errorDisplayField>
        <errorMessage>Approval Status Reason is Required if Approval Status = Denied or Delayed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_Name_is_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Contact_Name__c )</errorConditionFormula>
        <errorDisplayField>Contact_Name__c</errorDisplayField>
        <errorMessage>Required field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dedicated_Advisor_Required</fullName>
        <active>false</active>
        <errorConditionFormula>ISBLANK( Dedicated_Advisor__c )</errorConditionFormula>
        <errorDisplayField>Dedicated_Advisor__c</errorDisplayField>
        <errorMessage>Dedicated Advisor is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Membership_is_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Membership__c )</errorConditionFormula>
        <errorDisplayField>Membership__c</errorDisplayField>
        <errorMessage>Required field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NASA_Principal_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  ISBLANK( NASA_Principal__c ),
  ISPICKVAL( Member_Type__c, &quot;NASA&quot;)
)</errorConditionFormula>
        <errorDisplayField>NASA_Principal__c</errorDisplayField>
        <errorMessage>NASA Principal is required when Member Type is NASA</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Projected_Vetting_Date_is_Required</fullName>
        <active>true</active>
        <description>Required when Reference Status = Pending Approval and Reference Level = Pipeline</description>
        <errorConditionFormula>AND(
  ISPICKVAL( Reference_Status__c , &quot;Pending Approval&quot;),
  ISPICKVAL( Reference_Level__c , &quot;Pipeline&quot;) ,
  ISBLANK( Projected_Vetting_Date__c )
)</errorConditionFormula>
        <errorMessage>Projected Vetting Date is required when Reference Status is Pending Approval and Reference Level is Pipeline</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Promotion_Comments_Required</fullName>
        <active>true</active>
        <description>Requires a value if Promotion Status = â€˜Declinedâ€™</description>
        <errorConditionFormula>AND(
  ISPICKVAL( Promotion_Status__c , &quot;Declined&quot;),
  ISBLANK( Promotion_Comments__c )
)</errorConditionFormula>
        <errorMessage>Promotion Comments Required when Promotion Status = Declined</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reference_Contact_relate_to_Institution</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  NOT( ISBLANK( Contact_Name__c ) ),
  ISBLANK( Contact_Name__r.AccountId )
)</errorConditionFormula>
        <errorDisplayField>Contact_Name__c</errorDisplayField>
        <errorMessage>Please choose a Contact that is related to an Institution.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reference_Module_is_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Reference_Module__c )</errorConditionFormula>
        <errorMessage>Reference Module is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Reason_Comments_Required</fullName>
        <active>true</active>
        <description>Reason Comments are required when Status Reason = Other</description>
        <errorConditionFormula>AND(
  ISPICKVAL( Status_Reason__c , &quot;Other&quot;),
  ISBLANK( Status_Reason_Comments__c )
)</errorConditionFormula>
        <errorDisplayField>Status_Reason_Comments__c</errorDisplayField>
        <errorMessage>Status Reason Comments are required when Status Reason = Other</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Reason_Required</fullName>
        <active>true</active>
        <description>Require status reason when Reference Status = &quot;Former&quot; OR Promotion Status = &quot;Downgraded&quot;</description>
        <errorConditionFormula>AND(
  OR(
    ISPICKVAL( Reference_Status__c , &quot;Former&quot; ),
    ISPICKVAL( Reference_Status__c , &quot;Unqualified&quot; ),
    ISPICKVAL( Promotion_Status__c , &quot;Downgraded&quot; )
  ),
  ISPICKVAL( Status_Reason__c , &quot;&quot; )
)</errorConditionFormula>
        <errorDisplayField>Status_Reason__c</errorDisplayField>
        <errorMessage>Required when Reference Status = Former or Promotion Status = Downgraded</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Validate_Reference_Vetter_Requirednes</fullName>
        <active>true</active>
        <description>If Reference Level = Pipeline and Reference Status = Scheduled then Reference Vetter is required. Created for CR-20160307-9678.</description>
        <errorConditionFormula>AND (
	
	ISBLANK(Reference_Vetter__c),
	
	OR (

		AND (
			ISPICKVAL(Reference_Status__c, &quot;Scheduled&quot;),
			ISPICKVAL(Reference_Level__c, &quot;Pipeline&quot;)
		),

		AND (
			ISPICKVAL(Reference_Status__c, &quot;Vetted&quot;),
			ISPICKVAL(Reference_Level__c, &quot;Pipeline&quot;)
		),

		AND (
			ISPICKVAL(Reference_Status__c, &quot;Vetted&quot;),
			ISPICKVAL(Reference_Level__c, &quot;Gold Standard&quot;)
		),

		AND (
			ISPICKVAL(Reference_Status__c, &quot;Vetted&quot;),
			ISPICKVAL(Reference_Level__c, &quot;Ambassador&quot;)
		)
	)
)</errorConditionFormula>
        <errorDisplayField>Approval_Status_Date__c</errorDisplayField>
        <errorMessage>Reference Vetter is required when a Member is being vetted.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Vetting_Comments_Required</fullName>
        <active>true</active>
        <description>Required when Reference Status = â€˜Vettedâ€™</description>
        <errorConditionFormula>AND(
  ISPICKVAL( Reference_Status__c , &quot;Vetted&quot; ),
  ISBLANK( Vetting_Comments__c )
)</errorConditionFormula>
        <errorDisplayField>Vetting_Comments__c</errorDisplayField>
        <errorMessage>Vetting Comments are required when Reference Status = Vetted</errorMessage>
    </validationRules>
</CustomObject>
