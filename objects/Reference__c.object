        <externalId>false</externalId>
        <formula>IF( 
  OR( ISPICKVAL( Reference_Status__c , &quot;Pending&quot;), ISPICKVAL( Reference_Status__c , &quot;Scheduled&quot;) ),
  IMAGE(&quot;/resource/AttributeIcons/tag2_yellow16.png&quot;, &apos;None&apos;),
  IF( 
    AND( ISPICKVAL( Reference_Status__c , &quot;Vetted&quot;), ISPICKVAL( Reference_Level__c , &quot;Gold Standard&quot;) ),
    IMAGE(&quot;/resource/AttributeIcons/tag2_blue16.png&quot;, &apos;None&apos;),
    IF( 
      OR( ISPICKVAL( Reference_Status__c , &quot;Vetted&quot;), ISPICKVAL( Reference_Status__c , &quot;Ambassador&quot;) ),
      IMAGE(&quot;/resource/AttributeIcons/tag2_green16.png&quot;, &apos;None&apos;),
      IF( 
        OR( ISPICKVAL( Reference_Status__c , &quot;Unqualified&quot;), ISPICKVAL( Reference_Status__c , &quot;Former&quot;) ),
        IMAGE(&quot;/resource/AttributeIcons/tag2_red16.png&quot;, &apos;None&apos;),
        null
      )
    )
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
