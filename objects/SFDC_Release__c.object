    <validationRules>
        <fullName>Release_Cannot_Modify_LP_Integr_Fields</fullName>
        <active>true</active>
        <description>fields are not editable on eTech Dev events where External Id is populated.</description>
        <errorConditionFormula>AND (
    NOT( ISPICKVAL( $User.UserType, &quot;guest&quot;) ),
    NOT( ISBLANK( External_LP_Id__c ) ),
    OR(
        ISCHANGED( Name ),
        ISCHANGED( Target_Release_Date__c )
    )
)</errorConditionFormula>
        <errorMessage>Name and Target Release Date cannot be updated for Releases that were synced from LiquidPlanner. To update the Target Release Date on this Release record, Update the date on the Deployment Milestone in LP (ie: Event Type = &quot;Milestone - Deployment&quot;).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SFDC_Release_Notes_Required</fullName>
        <active>false</active>
        <description>must have release notes when
Deactivated for CR 9340 with Deployment Strategy Phase 1 updates.  If more process needs to be added, it can be done as an L2 or as part of Phase 2</description>
        <errorConditionFormula>AND(  
ISPICKVAL(Status__c ,&apos;Deployed&apos;), 
LEN( Release_Notes__c ) &lt; 5,
RecordType.DeveloperName = &apos;eTech Dev&apos;
)</errorConditionFormula>
        <errorDisplayField>Release_Notes__c</errorDisplayField>
        <errorMessage>You must document Release Notes for deployed releases</errorMessage>
    </validationRules>
