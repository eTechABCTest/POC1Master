        <errorConditionFormula>if(isblank( Client_Lost_Date__c) &amp;&amp; ispickval(Lost_To__c,&quot;&quot;) &amp;&amp; not(ispickval( Reason_for_Loss__c ,&quot;&quot;)), TRUE, 
if( ispickval(Lost_To__c,&quot;&quot;) &amp;&amp; not(isblank( Client_Lost_Date__c)) &amp;&amp; ispickval( Reason_for_Loss__c ,&quot;&quot;), TRUE, 
if(not(ispickval(Lost_To__c,&quot;&quot;)) &amp;&amp; isblank( Client_Lost_Date__c) &amp;&amp; ispickval( Reason_for_Loss__c ,&quot;&quot;), TRUE, 
if(not(isblank( Client_Lost_Date__c)) &amp;&amp; not(ispickval(Lost_To__c,&quot;&quot;)) &amp;&amp; ispickval( Reason_for_Loss__c ,&quot;&quot;), TRUE, 
if(isblank( Client_Lost_Date__c) &amp;&amp; not(ispickval(Lost_To__c,&quot;&quot;))&amp;&amp; not(ispickval( Reason_for_Loss__c ,&quot;&quot;)), TRUE, 
if(not(isblank( Client_Lost_Date__c)) &amp;&amp; not(ispickval( Reason_for_Loss__c ,&quot;&quot;)) &amp;&amp; ispickval(Lost_To__c,&quot;&quot;), TRUE, 
if(not(ispickval(Lost_To__c,&quot;&quot;))&amp;&amp; isblank( Client_Lost_Date__c) &amp;&amp; not(ispickval( Reason_for_Loss__c ,&quot;&quot;)), TRUE, FALSE)))))))</errorConditionFormula>
        <errorMessage>You must fill in Client Lost Date, Lost To and a Reason for Loss or leave all of them blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Engagement_status_validation</fullName>
