        <errorConditionFormula>ISCHANGED(Client_Status_New__c) &amp;&amp; ISPICKVAL(Client_Status_New__c, &quot;Secured&quot;) &amp;&amp; !$Permission.Invoicing_access</errorConditionFormula>
        <errorDisplayField>Client_Status_New__c</errorDisplayField>
