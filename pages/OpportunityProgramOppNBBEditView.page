    <!-- Script to populate Probability according to stage -->
    <script>
        
        //Function
        function showHideSections(elem) {
            
            //Get value of Stage
            var stageValue = elem.value;
            var flowControlVar = '';
            
            var flowControlVarCurrentValue = document.getElementById('pge:frm:pB:flowControl').value;
            
            //Get Flow Control variable value
            if(flowControlVarCurrentValue.indexOf('V1_INFO_OPEN_STAGE') != -1)
                flowControlVar = 'V1_INFO_OPEN_STAGE,';
            else if(flowControlVarCurrentValue.indexOf('V1_INFO_ADVANCE_STAGE') != -1)
                flowControlVar = 'V1_INFO_ADVANCE_STAGE,';
            else if(flowControlVarCurrentValue.indexOf('V1_INFO_Closed') != -1)
                flowControlVar = 'V1_INFO_Closed,';
            
            //Check for On Deck stage or Pipeline stage
            if(flowControlVarCurrentValue.indexOf('V1_INFO_Open_ON_DECK') != -1)
                flowControlVar = flowControlVar + 'V1_INFO_Open_ON_DECK,';
            else if(flowControlVarCurrentValue.indexOf('V1_INFO_ADVANCE_STAGE') != -1)
                flowControlVar = flowControlVar + 'V1_INFO_ADVANCE_STAGE,';
                
            
            //Check for value
            if(stageValue != null && stageValue != '' && stageValue != undefined) {
                
                
                if(stageValue != 'Closed Won' && stageValue != 'Closed Lost') {
                    
                    if(stageValue != 'Prospect' && stageValue != 'Outreach' && stageValue != 'Initial Visit Scheduled' && stageValue != 'Prewire') {
                        flowControlVar = flowControlVar + 'V2_MSND_ADVANCE_STAGE,';
                    }
                    
                    flowControlVar = flowControlVar + 'V2_OPEN_ADVANCE';
                }
                
                //Check for Close Opps
                if(stageValue == 'Closed Won' || stageValue == 'Closed Lost') {
                    flowControlVar = flowControlVar + 'V2_CLOSED_ADVANCE';
                }
            }
            
            //Check for value
            if(flowControlVar.localeCompare(document.getElementById('pge:frm:pB:flowControl').value) != 0) {
                document.getElementById('pge:frm:pB:flowControl').value = flowControlVar;
                reRenderAF(flowControlVar);
                
            }
        }
    </script>
    
