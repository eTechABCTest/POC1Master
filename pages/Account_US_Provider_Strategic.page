        j$(document).ready(function(){
            
            blockElement('#page\\:form\\:customizedRelatedLists');
            jsActivateRelatedLists();
            
            j$(window).scroll(function() {
                //check if your div is visible to user
                // CODE ONLY CHECKS VISIBILITY FROM TOP OF THE PAGE
                if (j$(window).scrollTop() + j$(window).height() >= j$(lazyLoadElement).offset().top) {
                    if(!loaded) {
                        //not in ajax.success due to multiple sroll events
                        //j$(lazyLoadElement).parent().attr('loaded', true);
                        loaded = true;
                        //ajax goes here
                        //by theory, this code still may be called several times
                        blockElement('#page\\:tabPanelContainer');
                        showTabPanels();
                    }
                }
            });
        });

