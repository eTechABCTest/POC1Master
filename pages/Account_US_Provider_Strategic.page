<apex:page standardController="Account" extensions="AccountController" id="page">

    <style>
        .activeTab {
            background-image: none;
            padding: 8px 5px 5px 5px;
            font-size: 11px !important;
            font-weight: bold;
            color: #2779AA;
            white-space: nowrap;
            
            /** gradient css **/
            background: -moz-linear-gradient(top, #3baae3 0%, #c4e6f7 100%);
            background: -o-linear-gradient(top, #3baae3 0%,#c4e6f7 100%);
            background: -ms-linear-gradient(top, #3baae3 0%,#c4e6f7 100%);
            filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#3baae3', endColorstr='#c4e6f7',GradientType=0 );
            background: #d7ebf9;
            background: -moz-linear-gradient(top, #d7ebf9 0%, #ffffff 100%);
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#d7ebf9), color-stop(100%,#ffffff));
            background: -webkit-linear-gradient(top, #d7ebf9 0%,#ffffff 100%);
            background: -o-linear-gradient(top, #d7ebf9 0%,#ffffff 100%);
            background: -ms-linear-gradient(top, #d7ebf9 0%,#ffffff 100%);
            background: linear-gradient(to bottom, #d7ebf9 0%,#ffffff 100%);
            filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#d7ebf9', endColorstr='#ffffff',GradientType=0 );
        }
        .inactiveTab {
            background-color: #D7EBF9;
            background-image: none;
            filter: none !important;
            color: #2779AA;
            padding: 5px;
            font-size: 11px;
            font-weight: bold;
            cursor: pointer;
            white-space: nowrap;
        }
        
        .rich-tabpanel-content { background-color: white; }
        
    </style>
	
    
	<div class="RLPanelShadow">
        <div class="RLPanel" id="RLPanel" onblur="sfdcPage.relatedListPanel.hideRLDelayed(null, 100)" onfocus="sfdcPage.relatedListPanel.showRL()" onmouseout="sfdcPage.relatedListPanel.hideRLDelayed(null, 100)" onmouseover="sfdcPage.relatedListPanel.showRL()">
        	<iframe frameborder="0" id="RLPanelFrame" name="RLPanelFrame" src="/emptyHtmlDoc.html" title="Hidden Frame - Ignore"></iframe>
    	</div>
    </div><div class="listHoverLinks"><span class="invisibleMinHeight">|</span></div>
    <script type="text/javascript">
    	try { sfdcPage.registerRelatedListPanel('RLPanel',false);} catch(e) {}
    </script>
    
    <!-- Account Detail Information -->
    <apex:detail subject="{!Account.Id}" inlineEdit="true" showChatter="true" relatedList="false" title="true" />
    
    <!-- Hidden Variable -->
    <div style="display:none;">
        <apex:outputField value="{!Account.RecordType.Name}" style="display:none;"/>
    </div>
    
    <!-- Related Lists -->
    <apex:form id="form">
            
        <!--Open Activities Action Function-->
        <apex:actionFunction name="showMoreChildActivities" action="{!showMoreChildActivities}" rerender="childActivityPanel, statusPanel" />
        <apex:actionFunction name="showTabPanels" rerender="tabPanelContainer" onComplete="unblockElement('#page\\:tabPanelContainer');" >
            <apex:param name="renderTabPanels" assignTo="{!renderTabPanels}" value="true" />
        </apex:actionFunction>
            
        <!--Show More Link Action Function : Restrictions-->
        <apex:actionFunction name="showMoreRestriction" action="{!showMoreRecords}" rerender="activeRestrictionPanel">
             <apex:param name="activeRestrictionObject" value="ActiveRestriction" assignTo="{!object_Name}"/>
        </apex:actionFunction>

        <!--Show More Link Action Function : Institution Roles-->
        <apex:actionFunction name="showMoreInstitutionRole" action="{!showMoreRecords}" rerender="accountRolePanel">
            <apex:param name="accountRoleObject" value="AccountRole" assignTo="{!object_Name}"/>
        </apex:actionFunction>
        
        <!-- DO NOT MOVE OR REMOVE THIS OUTPUT PANEL - IT IS USED TO LAZY LOAD THE APEX:TABPANELS -->
        <apex:outputPanel id="beginRenderingTabPanels" />
        
        <!--Show More Link Action Function : Institution Roles (Hospital System)-->
        <apex:actionFunction name="showMoreInstitutionRoleHospitalSystemType" action="{!showMoreRecords}" rerender="accountHospitalRolePanel">
            <apex:param name="accountRoleObject" value="AccountRole" assignTo="{!object_Name}"/>
        </apex:actionFunction>
        
        <!--Show More Link Action Function : Open NBB Opporutnities -->
        <apex:actionFunction name="showMoreOpenNBBOpportunity" action="{!showMoreRecords}" rerender="nBBPanel">
                <apex:param name="nbbObject" value="Opportunity" assignTo="{!object_Name}"/>
        </apex:actionFunction>
        
        <!--Show More Link Action Function : Memberships -->
        <apex:actionFunction name="showMoreMemberships" action="{!showMoreRecords}" rerender="membershipPanel">
            <apex:param name="membershipObject" value="Membership" assignTo="{!object_Name}"/>
        </apex:actionFunction>

        <!--Show More Link Action Function : Current Renewal Opportunities -->
        <apex:actionFunction name="showMoreCurrentRenewalOpportunities" action="{!showMoreRecords}" rerender="currentRenewalPanel">
            <apex:param name="currentRenewalOppObject" value="CurrentRenewalOpportunity" assignTo="{!object_Name}"/>
        </apex:actionFunction>

        <!--Show More Link Action Function : ContractsABC -->
        <apex:actionFunction name="showMoreContractsABC" action="{!showMoreRecords}" rerender="contractABCPanel">
            <apex:param name="contractABCObject" value="ContractABC" assignTo="{!object_Name}"/>
        </apex:actionFunction>
        
        <!-- Modified By - Abhinav Sharma - 05/21/2014 - CR-20140410-5043 -->
        <!--Do not remove it helpful in reRendering -->
        <apex:outputPanel id="statusPanel"/>
            
        <!--Show More Link Action Function : Open Child NBB Opportunities-->
        <apex:actionFunction name="switchToShowMoreForOpenChildNBB" action="{!showMoreRecords}" reRender="childOpenNBBPanel, statusPanel">
            <apex:param name="childNBBOppObject" value="OpenNBBChildOpportunity" assignTo="{!object_Name}"/>
        </apex:actionFunction>
        
        <!--Show More Link Action Function : Open Child Renewal Opportunities-->
        <apex:actionFunction name="switchToShowMoreForOpenChildRenewalOpp" action="{!showMoreRecords}" reRender="childRenewalOppPanel, statusPanel">
            <apex:param name="childRenewalOppObject" value="CurrentChildRenewalOpportunities" assignTo="{!object_Name}"/>
        </apex:actionFunction>
        
        <!--Show More Link Action Function : Active Child Contracts-->
        <apex:actionFunction name="switchToShowMoreForActiveChildContract" action="{!showMoreRecords}" reRender="childContractsPanel, statusPanel">
            <apex:param name="activeChildContractObject" value="ActiveChildContracts" assignTo="{!object_Name}"/>
        </apex:actionFunction>
        
        <apex:actionFunction name="jsActivateRelatedLists" reRender="customizedRelatedLists" oncomplete="" >
            <apex:param name="param1" value="true" assignTo="{!activateRelatedLists}"/>
        </apex:actionFunction>
        
        <apex:outputPanel id="customizedRelatedLists" layout="block" style="width:100%;min-height:250px;">
            <apex:outputPanel rendered="{!activateRelatedLists}">
            
                <!-- Restrictions -->
                <apex:outputPanel rendered="{!$ObjectType.Restricted_MtM__c.accessible}" id="activeRestrictionPanel">
                    <div class="bRelatedList" id="{!Account.Id}_00NC00000055pfV">
                        <a name="{!Account.Id}_00NC00000055pfV_target"></a>
                        <div class="listRelatedObject customnotabBlock">
                            <div class="bPageBlock brandSecondaryBrd secondaryPalette">
                                <div class="pbHeader">
                                    <table border="0" cellpadding="0" cellspacing="0">
                                        <tbody>
                                            <tr>
                                                <td class="pbTitle">
                                                    <img src="/s.gif" alt="" width="1" height="1" class="minWidth" title="" />
